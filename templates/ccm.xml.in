<?xml version="1.0" encoding="UTF-8" ?>
<ccm>
  <!--
     You may exclude multiple files and/or folders
  -->
  <exclude>
    <folder>obj</folder>
  </exclude>
    
  <!--
    Add a 'folder' element for each folder containing code to analyze.
    NOTE: Keep in mind that the path is relative to the location of the configuration file.
  -->
  <analyze>
    <folder>${DirSrc}</folder>
  </analyze>
  <recursive>yes</recursive>
  <outputXML>yes</outputXML>
  <numMetrics>50</numMetrics>
  <suppressMethodSignatures>no</suppressMethodSignatures>
</ccm>