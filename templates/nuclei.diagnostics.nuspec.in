<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Nuclei.Diagnostics</id>
    <version>${Version}</version>
    <authors>P. van der Velde</authors>
    <owners>P. van der Velde</owners>
    <licenseUrl>http://www.google.com</licenseUrl>
    <projectUrl>http://www.google.com</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Provides diagnostics methods and classes used to log issues and performance of
      deployed applications.
    </description>
    <releaseNotes>
      ${ReleaseNotes}
    </releaseNotes>
    <dependencies>
      ${Dependencies}
      <dependency id="Nuclei.Configuration" version="${Version}" />
    </dependencies>
  </metadata>
  <files>
    <file src="${DirBin}\Nuclei.Diagnostics.dll" target="lib\net45" />
    <file src="${DirBin}\Nuclei.Diagnostics.pdb" target="lib\net45" />
    <file src="${DirBin}\Nuclei.Diagnostics.xml" target="lib\net45" />
    <file src="${DirSrc}\Nuclei.Diagnostics\**\*.cs" target="src\Nuclei.Diagnostics" />
  </files>
</package>